# 高德地图 MCP 服务配置示例

# 地图服务提供商配置
MAP_PROVIDER=amap  # 可选值: "baidu" 或 "amap"

# 高德地图 API 密钥
AMAP_API_KEY=您在高德官网上申请的key

# 高德地图 MCP 服务配置
AMAP_MCP_MODE=http  # 可选值: "http" 或 "sse"
AMAP_MCP_HTTP_URL=https://mcp.amap.com/mcp
AMAP_MCP_SSE_URL=https://mcp.amap.com/sse

# 百度地图配置（备用）
BAIDU_MAPS_API_KEY=您的百度地图API密钥

# 其他配置
OPENAI_API_KEY=您的OpenAI API密钥
DATABASE_URL=postgresql://postgres:123456@localhost:5432/skyroam
REDIS_URL=redis://localhost:6379/0
